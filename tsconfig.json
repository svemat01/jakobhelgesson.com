{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$components/*": [
				"src/lib/components/*"
			],
			"$data/*": [
				"src/lib/data/*"
			],
			"$util/*": [
				"src/lib/util/*"
			],
			"$static/*": [
				"static/*"
			],
			"$modules/*": [
				"src/lib/modules/*"
			]
		}
	}
}